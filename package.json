{
	"name": "go-to-include",
	"displayName": "go-to-include",
	"description": "Easily navigate through your includes",
	"version": "0.0.1",
	"publisher": "rachidba",
	"repository": "https://github.com/Rachidba/vscode-go-to-include",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.goToInclude"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.goToInclude",
				"title": "Go to included file or directory"
			}
		],
		"keybindings": [
			{
			  "command": "extension.goToInclude",
			  "key": "ctrl+f1",
			  "mac": "cmd+f1"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2"
	}
}
